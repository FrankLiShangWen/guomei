*{
    box-sizing: border-box;
    margin: 0;
    padding: 0;
}
a{
    text-decoration: none;
}
ul>li{
    list-style: none;
}
#header,#nav,#section,#footer{
    width: 1200px;
    margin: 0 auto;
}
.lf{
    float: left;
}
.rt{
    float: right;
}
/****************header部分*********************/

#header{
    height: 40px;
    background: #F8F8F8;
    border-bottom: 1px solid #E6E6E6;
    border-left: 1px solid transparent;
}
#header a{
    line-height: 40px;
    color: #888;
    margin: 0 10px;
    display: inline-block;
    height: 40px;
}
#header a:hover{
    color: #CC0000;
}
#header>div.lf>b{
    display: inline-block;
    height: 40px;
    font-weight: normal;
    padding: 0 10px;
    position: relative;
}


#header>div.lf>b>span{
    display: inline-block;
    background: url(../image/ui.png) no-repeat -60px -481px;
    width: 14px;
    height: 8px;
}
/***********************国美会员弹框*/
#header>div.lf>b>div.vip{
    width: 400px;
    height: 270px;
    border: 1px solid #E6E6E6;
    border-top: 0;
    display: none;
}

#header>div.lf>b:hover div.vip{
    display: block;
    z-index: 100;
}
/*#user{*/
    /*display: inline-block;*/
    /**/
/*}*/
#user{
	color:#f00;
}
#header>div.lf>b:hover span,{
    /*改变箭头*/
    background: url(../image/ui.png) no-repeat -60px -493px;
}
#header>div.lf>b:hover,.web_nav:hover{
    background: #fff;
    border-left-color: #E6E6E6;
    border-right-color: #E6E6E6;
    border-color:#E6E6E6;
}
#header>div.lf>b{
    border-left: 1px solid #F8F8F8;
    border-right: 1px solid #F8F8F8;
}
#header .vip{
    position: absolute;
    top: 40px;
    left: -1px;
    background: #fff;
    text-align: center;
}
#header .vip>div.denglu{
    border-bottom: 1px solid #E6E6E6;
    margin: 10px;
    text-align: center;

}

#header .vip>div.denglu>i{
    display: inline-block;
    width: 55px;
    height: 55px;

    background: url(../image/ui.png) no-repeat 0 -337px ;
    margin: 15px 0 0 15px;
}
#header .vip>div>b{
    display: inline-block;
}
.vip>p>a:first-child{
    color: #5E5E5E;
}
.vip>p>a:nth-child(2){
    color: #CCCCCC;
}
.vip>p:after{
    content: "";
    display: table;
    clear: both;
}
.vip_privilege>dl{
   display: inline-block;

}
.vip_privilege img:hover{
    opacity: .5;
}
.vip_privilege>i:first-child{
    display: inline-block;
    width: 17px;
    height: 30px;
    background: url(../image/ui.png) no-repeat -60px -357px;
    margin-bottom: 23px;
    cursor: pointer;
}
.vip_privilege>i:first-child:hover{
    background: url(../image/ui.png) no-repeat -60px -513px;
}
.vip_privilege>i:last-child{
    display: inline-block;
    width: 17px;
    height: 30px;
    background: url(../image/ui.png) no-repeat -76px -357px;
    margin-bottom: 23px;
    cursor: pointer;
}
.vip_privilege>i:last-child:hover{
    background: url(../image/ui.png) no-repeat -76px -513px;
}

/***********我的国美************/
.my_gm>span,.phone_gm>span,.web_nav>span{
    display: inline-block;
    width: 13px;
    height: 8px;
    background: url(../image/ui.png) no-repeat -60px -481px;
}
.my_gm:hover span,.phone_gm:hover span,.web_nav:hover span{
    /*改变箭头*/
    background: url(../image/ui.png) no-repeat -60px -493px;
}
.my_gm{
    display: inline-block;
    height: 40px;
    position: relative;
    border-left: 1px solid #F8F8F8;
    border-right: 1px solid #F8F8F8;
    padding-right: 10px;
}
.my_gm_list{
    width: 300px;
    min-height: 217px;
    border: 1px solid #E6E6E6;
    border-top: 0;
    position: absolute;
    top: 40px;
    left: -1px;
    display: none;
    background: #fff;
}
.my_gm_list>div:first-child{
    border-bottom: 1px solid #E6E6E6;
    content: "";
    display: table;
    margin: 10px;
}
.my_gm_list>div:first-child li{
    float: left;
    margin-right: 40px;
}
.my_gm_list>div:first-child b{
    margin-left: 10px;
}
.my_gm_list>div:nth-child(2) li{
    float: left;
    width: 68px;
    text-align: center;
}
.my_gm_list>div:nth-child(2){
    content: "";
    display: table;
}
.my_gm_list div:first-child b{
    color: #968888;
}
#header .my_gm_list div:first-child ul>a{
    color:#006699;
    font-weight: bold;
}

/*我的国美弹框*/
.my_gm:hover{
    background: #fff;
    border-color: #E6E6E6;
}
.my_gm:hover .my_gm_list{
    display: inline-block;
    z-index: 100;
}
.my_gm_list div:nth-child(3){
    background: #F8F8F8;
    height: 132px;
}
.my_gm_list div:nth-child(3) p{
    height: 50px;
    line-height: 50px;
    padding-left: 20px;
}
.my_gm_list div:nth-child(3) i{
    display: inline-block;
    width: 55px;
    height: 55px;
    background: url("../image/ui.png") no-repeat 0 -396px;
    margin-left: 13px;
}

/***************服务中心****************/
.service>i,.service_list a:last-child i{
    display: inline-block;
    width: 20px;
    height: 20px;
    background: url(../image/ui.png) no-repeat 0 -513px;
    margin-bottom: -6px;
}
.service,.phone_gm{
    display: inline-block;
    height: 40px;
    line-height: 40px;
    position: relative;
    border-left: 1px solid #F8F8F8;
    border-right: 1px solid #F8F8F8;
